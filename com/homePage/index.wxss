/* com/homePage/index.wxss */
page {
  height: 100%;
  position: relative;
  overflow: hidden;
  box-sizing: border-box;
}

.view-page {
  height: 100%;
  width: 100%;
  left: 0;
  top: 0;
  box-sizing: border-box;
  position: absolute;
  overflow: hidden;
}

.page-content {
  box-sizing: border-box;
  position: relative;
  overflow-y: auto;
}
.btn{
  margin-top: 20px;
}

.container {
  box-sizing: border-box;
  width: 100%;
  height: 100%;
  background: #eee;
  text-align: center;
}
